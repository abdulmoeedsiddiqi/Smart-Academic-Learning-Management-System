<!-- Header Banner with Background Image -->
<div class="w-full h-64 bg-cover bg-center relative -mt-8 -mx-4" style="background-image: url('/images/dark-gradient-bg.jpg')">
  <div class="absolute inset-0 bg-gradient-to-r from-indigo-800/90 via-indigo-700/90 to-indigo-600/90"></div>
  <div class="container mx-auto px-6 relative z-10 h-full flex items-center">
    <div class="flex items-center justify-between w-full">
      <div class="flex-1">
        <h1 class="text-4xl md:text-5xl font-bold text-white mb-2 drop-shadow-lg">
          Welcome, <%= user.firstName %>!
          <div class="h-1 w-24 bg-emerald-400 mt-2 rounded-full"></div>
        </h1>
        <p class="text-lg text-white font-medium">Your learning journey dashboard</p>
      </div>
      <div class="flex-shrink-0 hidden md:block">
        <img src="/images/giki.png" alt="Student" class="w-32 h-32 rounded-full border-2 border-emerald-400" />
      </div>
    </div>
  </div>
</div>

<!-- Dashboard Content -->
<div class="container mx-auto px-4 -mt-10">
  <!-- Quick Stats Cards -->
  <div class="grid grid-cols-2 md:grid-cols-4 gap-4 mb-8">
    <div class="bg-white rounded-xl shadow-xl p-5 border-t-2 border-indigo-500 transform hover:translate-y-[-5px] transition-all duration-300">
      <div class="flex items-center">
        <div class="p-3 rounded-lg bg-indigo-50 mr-4">
          <svg class="w-7 h-7 text-indigo-700" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 6.253v13m0-13C10.832 5.477 9.246 5 7.5 5S4.168 5.477 3 6.253v13C4.168 18.477 5.754 18 7.5 18s3.332.477 4.5 1.253m0-13C13.168 5.477 14.754 5 16.5 5c1.747 0 3.332.477 4.5 1.253v13C19.832 18.477 18.247 18 16.5 18c-1.746 0-3.332.477-4.5 1.253"></path>
          </svg>
        </div>
        <div>
          <p class="text-sm text-gray-600">Courses</p>
          <p class="text-3xl font-bold text-gray-900"><%= enrollments.length %></p>
        </div>
      </div>
    </div>
    
    <div class="bg-white rounded-xl shadow-xl p-5 border-t-2 border-emerald-500 transform hover:translate-y-[-5px] transition-all duration-300">
      <div class="flex items-center">
        <div class="p-3 rounded-lg bg-emerald-50 mr-4">
          <svg class="w-7 h-7 text-emerald-700" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5H7a2 2 0 00-2 2v12a2 2 0 002 2h10a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2m-3 7h3m-3 4h3m-6-4h.01M9 16h.01"></path>
          </svg>
        </div>
        <div>
          <p class="text-sm text-gray-600">Assignments</p>
          <p class="text-3xl font-bold text-gray-900"><%= pendingAssignments.length %></p>
        </div>
      </div>
    </div>
    
    <div class="bg-white rounded-xl shadow-xl p-5 border-t-2 border-green-500 transform hover:translate-y-[-5px] transition-all duration-300">
      <div class="flex items-center">
        <div class="p-3 rounded-lg bg-green-50 mr-4">
          <svg class="w-7 h-7 text-green-700" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"></path>
          </svg>
        </div>
        <div>
          <p class="text-sm text-gray-600">Completed</p>
          <p class="text-3xl font-bold text-gray-900"><%= enrollments.filter(e => e.status === 'completed').length %></p>
        </div>
      </div>
    </div>
    
    <div class="bg-white rounded-xl shadow-xl p-5 border-t-2 border-amber-500 transform hover:translate-y-[-5px] transition-all duration-300">
      <div class="flex items-center">
        <div class="p-3 rounded-lg bg-amber-50 mr-4">
          <svg class="w-7 h-7 text-amber-700" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 3.055A9.001 9.001 0 1020.945 13H11V3.055z"></path>
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M20.488 9H15V3.512A9.025 9.025 0 0120.488 9z"></path>
          </svg>
        </div>
        <div>
          <p class="text-sm text-gray-600">Active</p>
          <p class="text-3xl font-bold text-gray-900"><%= enrollments.filter(e => e.status === 'active').length %></p>
        </div>
      </div>
    </div>
  </div>

  <div class="grid grid-cols-1 lg:grid-cols-3 gap-6">
    <div class="lg:col-span-2">
      <!-- Upcoming Assignments Section - Updated with file download functionality -->
      <div class="bg-white rounded-xl shadow-xl p-6 mb-6 border-l-4 border-amber-500">
        <h2 class="text-2xl font-bold text-gray-900 mb-5 flex items-center">
          <svg class="w-6 h-6 mr-3 text-amber-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"></path>
          </svg>
          Upcoming Deadlines
        </h2>
        <% if (pendingAssignments.length > 0) { %>
          <div class="space-y-3">
            <% pendingAssignments.forEach(assignment => { %>
              <% 
                const dueDate = new Date(assignment.dueDate);
                const today = new Date();
                const daysLeft = Math.ceil((dueDate - today) / (1000 * 60 * 60 * 24));
                let bgColor = 'bg-gray-50';
                let borderColor = 'border-l-amber-400';
                
                if (daysLeft < 3) {
                  bgColor = 'bg-red-50';
                  borderColor = 'border-l-red-500';
                } else if (daysLeft < 7) {
                  bgColor = 'bg-amber-50';
                  borderColor = 'border-l-amber-500';
                }
              %>
              <div class="flex items-start p-4 <%= bgColor %> rounded-lg border-l-4 <%= borderColor %> hover:bg-opacity-70 transition-all duration-200">
                <div class="mr-4 <%= daysLeft < 3 ? 'text-red-700' : 'text-amber-700' %> mt-1">
                  <svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z" />
                  </svg>
                </div>
                <div class="flex-1">
                  <div class="flex items-start justify-between">
                    <div>
                      <p class="font-medium text-lg text-gray-900">
                        <% if (assignment.assignmentNumber) { %>
                          Assignment <%= assignment.assignmentNumber %>: 
                        <% } %>
                        <%= assignment.title %>
                      </p>
                      <p class="text-sm text-gray-600"><%= assignment.Course.name %></p>
                    </div>
                    <div class="text-right flex-shrink-0">
                      <p class="text-sm <%= daysLeft < 3 ? 'text-red-700' : 'text-amber-700' %>">Due: <%= new Date(assignment.dueDate).toLocaleDateString() %></p>
                      <p class="text-xs <%= daysLeft < 3 ? 'text-red-700 font-bold' : 'text-gray-500' %>"><%= daysLeft %> days left</p>
                    </div>
                  </div>
                  
                  <% if (assignment.filePath) { %>
                    <div class="mt-2 flex items-center">
                      <svg class="w-5 h-5 text-indigo-600 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 21h10a2 2 0 002-2V9.414a1 1 0 00-.293-.707l-5.414-5.414A1 1 0 0012.586 3H7a2 2 0 00-2 2v14a2 2 0 002 2z"></path>
                      </svg>
                      <a href="<%= assignment.filePath %>" target="_blank" class="text-sm text-indigo-600 hover:text-indigo-800 font-medium flex items-center">
                        <span class="mr-2">
                          <%= assignment.fileOriginalName ? assignment.fileOriginalName : "Assignment PDF" %>
                        </span>
                        <span class="bg-indigo-100 text-indigo-800 text-xs px-2 py-1 rounded-full flex items-center">
                          <svg class="w-3 h-3 mr-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 16v1a3 3 0 003 3h10a3 3 0 003-3v-1m-4-4l-4 4m0 0l-4-4m4 4V4"></path>
                          </svg>
                          Download
                        </span>
                      </a>
                    </div>
                  <% } %>
                </div>
              </div>
            <% }); %>
          </div>
        <% } else { %>
          <div class="bg-gray-50 rounded-lg p-6 text-center">
            <svg class="w-16 h-16 text-gray-400 mx-auto mb-3" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"></path>
            </svg>
            <p class="text-gray-600">No pending assignments. You're all caught up!</p>
          </div>
        <% } %>
      </div>

      <!-- Recent Grades -->
      <div class="bg-white rounded-xl shadow-xl mb-6 border-l-4 border-violet-500">
        <div class="p-6">
          <h2 class="text-2xl font-bold text-gray-900 mb-5 flex items-center">
            <svg class="w-6 h-6 mr-3 text-violet-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"></path>
            </svg>
            Recent Grades
          </h2>
        </div>
        <% if (recentGrades.length > 0) { %>
          <div class="overflow-x-auto">
            <table class="min-w-full divide-y divide-gray-200">
              <thead class="bg-gray-50">
                <tr>
                  <th class="px-6 py-3 text-left text-xs font-medium text-gray-700 uppercase tracking-wider">Assignment</th>
                  <th class="px-6 py-3 text-left text-xs font-medium text-gray-700 uppercase tracking-wider">Course</th>
                  <th class="px-6 py-3 text-left text-xs font-medium text-gray-700 uppercase tracking-wider">Grade</th>
                  <th class="px-6 py-3 text-left text-xs font-medium text-gray-700 uppercase tracking-wider">Date</th>
                </tr>
              </thead>
              <tbody class="divide-y divide-gray-200 bg-white">
                <% recentGrades.forEach(grade => { %>
                  <tr class="hover:bg-gray-50 transition-colors duration-150">
                    <td class="px-6 py-4 whitespace-nowrap">
                      <div class="text-sm font-medium text-gray-900"><%= grade.Assignment ? grade.Assignment.title : 'N/A' %></div>
                    </td>
                    <td class="px-6 py-4 whitespace-nowrap">
                      <div class="text-sm text-gray-700"><%= grade.Course ? grade.Course.name : 'N/A' %></div>
                    </td>
                    <td class="px-6 py-4 whitespace-nowrap">
                      <% if (grade.Assignment) { %>
                        <% 
                          const percentage = (grade.value / (grade.Assignment.totalMarks || 100)) * 100;
                          let gradeColor = 'text-gray-700';
                          let gradeBg = 'bg-gray-100';
                          if (percentage >= 90) {
                            gradeColor = 'text-green-800';
                            gradeBg = 'bg-green-100';
                          } else if (percentage >= 80) {
                            gradeColor = 'text-indigo-800';
                            gradeBg = 'bg-indigo-100';
                          } else if (percentage >= 70) {
                            gradeColor = 'text-amber-800';
                            gradeBg = 'bg-amber-100';
                          } else if (percentage < 60) {
                            gradeColor = 'text-red-800';
                            gradeBg = 'bg-red-100';
                          }
                          
                          let gradeText = percentage >= 90 ? 'A' : 
                            percentage >= 80 ? 'B' :
                            percentage >= 70 ? 'C' :
                            percentage >= 60 ? 'D' : 'F';
                        %>
                        <div class="flex items-center">
                          <div class="mr-3">
                            <div class="text-sm text-gray-900"><%= grade.value %>/<%= grade.Assignment.totalMarks || 100 %></div>
                            <div class="text-xs text-gray-600"><%= percentage.toFixed(1) %>%</div>
                          </div>
                          <span class="px-2 py-1 text-xs font-bold rounded <%= gradeBg %> <%= gradeColor %>">
                            <%= gradeText %>
                          </span>
                        </div>
                      <% } else { %>
                        <div class="text-sm text-gray-900"><%= grade.value %>/<%= grade.totalMarks || 100 %></div>
                      <% } %>
                    </td>
                    <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-600">
                      <%= new Date(grade.updatedAt).toLocaleDateString() %>
                    </td>
                  </tr>
                <% }); %>
              </tbody>
            </table>
          </div>
        <% } else { %>
          <div class="p-6">
            <p class="text-gray-600">No grades available yet.</p>
          </div>
        <% } %>
      </div>
    </div>
    
    <div class="lg:col-span-1">
      <!-- Attendance Stats -->
      <div class="bg-white rounded-xl shadow-xl p-6 mb-6 border-l-4 border-indigo-500">
        <h2 class="text-2xl font-bold text-gray-900 mb-5 flex items-center">
          <svg class="w-6 h-6 mr-3 text-indigo-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5H7a2 2 0 00-2 2v12a2 2 0 002 2h10a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2"></path>
          </svg>
          Attendance
        </h2>
        <% if (attendanceStats && attendanceStats.length > 0) { %>
          <div class="space-y-4">
            <% attendanceStats.forEach(stat => { %>
              <div class="bg-gray-50 rounded-lg p-4 hover:bg-gray-100 transition-colors duration-200">
                <h3 class="font-bold text-lg mb-3 text-gray-900"><%= stat.courseName %></h3>
                <div class="mb-2 flex items-center justify-between">
                  <div class="w-full h-2 bg-gray-200 rounded-full overflow-hidden mr-3 flex-grow">
                    <% 
                      let barColor = 'bg-gradient-to-r from-red-400 to-red-500';
                      if (stat.percentage >= 75) {
                        barColor = 'bg-gradient-to-r from-green-400 to-indigo-400';
                      } else if (stat.percentage >= 60) {
                        barColor = 'bg-gradient-to-r from-amber-400 to-orange-400';
                      }
                    %>
                    <div class="h-full <%= barColor %>" style="width: <%= typeof stat.percentage === 'number' ? stat.percentage : 0 %>%"></div>
                  </div>
                  <span class="text-lg font-bold <%= stat.percentage >= 75 ? 'text-green-600' : (stat.percentage >= 60 ? 'text-amber-600' : 'text-red-600') %>">
                    <%= typeof stat.percentage === 'number' ? stat.percentage : 0 %>%
                  </span>
                </div>
                <p class="text-sm text-gray-600">Attended <%= stat.presentClasses %> out of <%= stat.totalClasses %> classes</p>
              </div>
            <% }); %>
          </div>
        <% } else { %>
          <div class="bg-gray-50 rounded-lg p-6 text-center">
            <svg class="w-16 h-16 text-gray-400 mx-auto mb-3" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z"></path>
            </svg>
            <p class="text-gray-600">No attendance records available.</p>
          </div>
        <% } %>
      </div>
    </div>
  </div>

  <!-- My Courses -->
  <div class="mb-8">
    <h2 class="text-2xl font-bold text-gray-900 mb-5 flex items-center">
      <svg class="w-6 h-6 mr-3 text-indigo-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 6.253v13m0-13C10.832 5.477 9.246 5 7.5 5S4.168 5.477 3 6.253v13C4.168 18.477 5.754 18 7.5 18s3.332.477 4.5 1.253m0-13C13.168 5.477 14.754 5 16.5 5c1.747 0 3.332.477 4.5 1.253v13C19.832 18.477 18.247 18 16.5 18c-1.746 0-3.332.477-4.5 1.253"></path>
      </svg>
      My Courses
    </h2>
    <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">
      <% if (enrollments.length > 0) { %>
        <% enrollments.forEach(enrollment => { %>
          <% 
            let borderColor = 'border-indigo-500';
            let statusColor = 'bg-indigo-100 text-indigo-800';
            
            if (enrollment.status === 'completed') {
              borderColor = 'border-green-500';
              statusColor = 'bg-green-100 text-green-800';
            }
          %>
          <div class="bg-white rounded-xl shadow-xl overflow-hidden transform hover:translate-y-[-5px] transition-all duration-300 border-t-2 <%= borderColor %>">
            <div class="p-6">
              <div class="flex justify-between items-start mb-4">
                <h3 class="font-bold text-xl text-gray-900"><%= enrollment.Course.name %></h3>
                <span class="px-3 py-1 text-xs rounded-full <%= statusColor %>">
                  <%= enrollment.status.charAt(0).toUpperCase() + enrollment.status.slice(1) %>
                </span>
              </div>
              <p class="text-sm text-indigo-600 mb-3 font-mono"><%= enrollment.Course.courseCode %></p>
              <% if (enrollment.Course.instructor && enrollment.Course.instructor.User) { %>
                <div class="flex items-center mb-3 text-gray-700">
                  <svg class="w-4 h-4 mr-2 text-gray-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z"></path>
                  </svg>
                  <span class="text-sm">
                    <%= enrollment.Course.instructor.User.firstName %> <%= enrollment.Course.instructor.User.lastName %>
                  </span>
                </div>
              <% } %>
              <p class="text-sm text-gray-600 mb-5 h-12 overflow-hidden">
                <%= enrollment.Course.description ? enrollment.Course.description.substring(0, 100) + (enrollment.Course.description.length > 100 ? '...' : '') : 'No description available' %>
              </p>
              <a href="/student/courses/<%= enrollment.Course.id %>" class="inline-block bg-indigo-600 hover:bg-indigo-700 text-white px-4 py-2 rounded-lg text-sm transition duration-150 ease-in-out">
                <span class="flex items-center">
                  <span>View Course</span>
                  <svg class="w-4 h-4 ml-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M14 5l7 7m0 0l-7 7m7-7H3"></path>
                  </svg>
                </span>
              </a>
            </div>
          </div>
        <% }); %>
      <% } else { %>
        <div class="col-span-full bg-white rounded-xl p-8 text-center shadow-xl">
          <svg class="w-20 h-20 text-gray-300 mx-auto mb-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 6.253v13m0-13C10.832 5.477 9.246 5 7.5 5S4.168 5.477 3 6.253v13C4.168 18.477 5.754 18 7.5 18s3.332.477 4.5 1.253m0-13C13.168 5.477 14.754 5 16.5 5c1.747 0 3.332.477 4.5 1.253v13C19.832 18.477 18.247 18 16.5 18c-1.746 0-3.332.477-4.5 1.253"></path>
          </svg>
          <p class="text-gray-600 text-lg">You are not enrolled in any courses yet.</p>
          <a href="/student/browse-courses" class="mt-4 inline-flex items-center px-4 py-2 bg-indigo-600 hover:bg-indigo-700 text-white rounded-lg transition duration-150">
            Browse Available Courses
            <svg class="w-4 h-4 ml-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M14 5l7 7m0 0l-7 7m7-7H3"></path>
            </svg>
          </a>
        </div>
      <% } %>
    </div>
  </div>

  <!-- Course Discovery Section -->
  <div class="mb-8 relative overflow-hidden bg-gradient-to-r from-indigo-500 via-purple-500 to-pink-500 rounded-2xl shadow-xl">
    <div class="absolute inset-0 bg-black bg-opacity-20"></div>
    <div class="relative p-8 md:p-10">
      <div class="grid grid-cols-1 md:grid-cols-3 gap-8">
        <div class="md:col-span-2">
          <h2 class="text-3xl font-bold text-white mb-4">Discover New Courses</h2>
          <p class="text-white text-opacity-90 mb-6 text-lg">
            Explore courses taught by expert instructors and expand your knowledge. New courses are added regularly to help you grow your skills.
          </p>
          <div class="flex flex-wrap gap-3 mb-6">
            <span class="bg-white bg-opacity-20 text-white px-3 py-1 rounded-full text-sm font-medium">Computer Science</span>
            <span class="bg-white bg-opacity-20 text-white px-3 py-1 rounded-full text-sm font-medium">Mathematics</span>
            <span class="bg-white bg-opacity-20 text-white px-3 py-1 rounded-full text-sm font-medium">Engineering</span>
            <span class="bg-white bg-opacity-20 text-white px-3 py-1 rounded-full text-sm font-medium">Business</span>
            <span class="bg-white bg-opacity-20 text-white px-3 py-1 rounded-full text-sm font-medium">Arts & Humanities</span>
          </div>
          <a href="/student/browse-courses" class="inline-flex items-center px-6 py-3 bg-white text-indigo-700 rounded-lg font-medium shadow-md hover:bg-gray-100 transition duration-150">
            Browse All Courses
            <svg class="w-5 h-5 ml-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3"></path>
            </svg>
          </a>
        </div>
        <div class="hidden md:flex md:items-center md:justify-center">
          <div class="bg-white bg-opacity-20 w-60 h-60 rounded-full flex items-center justify-center">
            <div class="bg-white bg-opacity-30 w-48 h-48 rounded-full flex items-center justify-center">
              <div class="bg-white w-36 h-36 rounded-full flex items-center justify-center shadow-lg">
